<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Svg</id>
        <version>$version$</version>
        <title>Portable SVG Rendering Library</title>
        <authors>gentledepp,bruzkovsky,davescriven,jvenema,owaits,ddpruitt,Ralf1108,Tebjan Halm,and others</authors>
        <owners>gentledepp,bruzkovsky</owners>
        <licenseUrl>http://opensource.org/licenses/MS-PL.html</licenseUrl>
        <projectUrl>https://github.com/gentledepp/SVG</projectUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>
			Public fork of the C# SVG rendering library on codeplex: https://github.com/vvvv/SVG
			The main goal was to make it platform independant to run it on Xamarin iOS, Android and UWP devices. 
		</description>
        <summary>Open source library to load/save and render SVG vector graphics.</summary>
        <releaseNotes>merged github pull requests</releaseNotes>
        <tags>svg, vector graphics, rendering</tags>
        <dependencies>
			<dependency id="SkiaSharp" version="1.56.2" exclude="Build,Analyzers" />
		</dependencies>
    </metadata>
    <files>
		<!-- platform independent assemblies --> 
        <file src="..\Svg.Core\bin\Release\Svg.dll" target="lib\portable45-net45+win8+wpa81\Svg.dll" />
		
        <!-- iOS reference assemblies -->
        <file src="..\Svg.iOS\bin\Release\Svg.dll" target="lib\Xamarin.iOS10\Svg.dll" />
		
        <!-- Android reference assemblies -->
        <file src="..\Svg.Droid\bin\Release\Svg.dll" target="lib\MonoAndroid403\Svg.dll" />
		
        <!-- UWP reference assemblies -->
        <file src="..\Svg.UWP\bin\Release\Svg.dll" target="lib\UAP10\Svg.dll" />
		
        <!-- Windows reference assemblies -->
        <file src="..\Svg.Win\bin\Release\Svg.dll" target="lib\net452\Svg.dll" />
    </files>
</package>